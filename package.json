{
  "name": "@unicaclub/wppconnect-server",
  "version": "2.8.6",
  "description": "WPPConnect Server com Sistema de Automação Avançado - Enterprise WhatsApp Automation Platform",
  "main": "dist/server.js",
  "author": "Unicaclub",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Unicaclub/Wppconect.git"
  },
  "homepage": "https://github.com/Unicaclub/Wppconect",
  "bugs": {
    "url": "https://github.com/Unicaclub/Wppconect/issues"
  },
  "scripts": {
    "build:types": "tsc",
    "build:js": "rimraf dist && babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps inline --copy-files",
    "build": "npm run build:types && npm run build:js",
    "build:simple": "tsc && babel src --out-dir dist --extensions \".ts,.tsx\" --copy-files",
    "railway:build": "npm run build:simple",
    "railway:start": "npm start",
    "dev": "tsx watch src/server.ts",
    "start": "node ./dist/server.js",
    "start:production": "NODE_ENV=production npm start",
    "setup:automation": "node setup-automation.js",
    "test:automation": "node test-automation.js",
    "healthcheck": "curl -f http://localhost:${PORT:-21465}/healthz || exit 1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.855.0",
    "@wppconnect-team/wppconnect": "^1.37.3",
    "archiver": "^7.0.1",
    "axios": "^1.11.0",
    "bcrypt": "^5.1.1",
    "buffer-to-stream": "1.0.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.6.1",
    "express": "4.21.2",
    "express-query-boolean": "^2.0.0",
    "form-data": "^4.0.4",
    "json-mapper-json": "^1.3.3",
    "merge-deep": "^3.0.3",
    "moment": "^2.30.1",
    "mongoose": "^8.16.5",
    "multer": "^1.4.5-lts.2",
    "qrcode": "^1.5.4",
    "socket.io": "^4.8.1",
    "swagger-autogen": "^2.23.7",
    "swagger-ui-express": "^5.0.1",
    "unzipper": "^0.12.3",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.28.0",
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-typescript": "^7.27.1",
    "@types/archiver": "^6.0.3",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.19",
    "@types/express": "4.17.23",
    "@types/multer": "^1.4.13",
    "@types/node": "^22.17.0",
    "@types/qrcode": "^1.5.5",
    "rimraf": "^5.0.10",
    "tsx": "^4.20.3",
    "typescript": "^5.9.2"
  },
  "keywords": [
    "whatsapp",
    "automation",
    "wppconnect",
    "chatbot",
    "messaging",
    "api",
    "webhook",
    "railway",
    "enterprise"
  ]
}